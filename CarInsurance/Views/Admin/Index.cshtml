@using CarInsurance.ViewModels
@model List<InsureeVM>
@{
    Layout = null;
}


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <div>
        @foreach (var insuree in Model)
        {
            
            {
                
                <div class="row mb-3">
                    <div class="col-2">
                        @insuree.FirstName
                    </div>
                    <div class="col">
                        @insuree.LastName
                    </div>
                    <div class="col-4">
                        @insuree.EmailAddress
                    </div>
                    <div class="col-4">
                        @insuree.Quote
                    </div>


                </div>
            }
        }
    </div>
</body>
</html>
